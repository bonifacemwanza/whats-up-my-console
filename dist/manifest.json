{
  "manifest_version": 3,
  "name": "Whats up my console",
  "version": "1.0",
  "description": "This extension enables developers to read their consoled  messages and errors in the IDE terminal messages",
  "permissions": [
    "tabs",
    "debugger"
  ],
  "action": {
    "default_popup": "index.html",  
    "default_icon": {
      "16": "assets/icon16.png", 
      "48": "assets/icon48.png",  
      "128": "assets/icon128.png"  
    }
  },
  "icons": {
    "16": "assets/icon16.png",  
    "48": "assets/icon48.png",  
    "128": "assets/icon128.png" 
  },
  "background": {
    "service_worker": "background.js"
  }

 
}
